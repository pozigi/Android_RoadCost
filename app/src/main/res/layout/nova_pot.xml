<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.704501"
        tools:layout_editor_absoluteY="360dp"
        tools:layout_editor_absoluteX="0dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline4"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.15459883"
        tools:layout_editor_absoluteY="79dp"
        tools:layout_editor_absoluteX="0dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline11"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="181dp"
        tools:layout_editor_absoluteX="0dp"
        app:layout_constraintGuide_percent="0.35420743" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline12"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="41dp"
        app:layout_constraintGuide_percent="0.106770836" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline13"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="346dp"
        app:layout_constraintGuide_percent="0.9010417" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline14"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="258dp"
        tools:layout_editor_absoluteX="0dp"
        app:layout_constraintGuide_percent="0.50489235" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline15"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="463dp"
        tools:layout_editor_absoluteX="0dp"
        app:layout_constraintGuide_percent="0.90606654" />

    <Button
        android:text="START"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/button4"
        app:layout_constraintRight_toLeftOf="@+id/guideline13"
        android:layout_marginEnd="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline12"
        app:layout_constraintTop_toTopOf="@+id/guideline4"
        app:layout_constraintBottom_toTopOf="@+id/guideline11"
        android:layout_marginBottom="8dp"
        android:background="@android:color/holo_green_dark" />

    <Button
        android:text="STOP"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/button5"
        app:layout_constraintRight_toLeftOf="@+id/guideline13"
        android:layout_marginEnd="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline12"
        app:layout_constraintTop_toTopOf="@+id/guideline14"
        app:layout_constraintBottom_toTopOf="@+id/guideline2"
        android:layout_marginBottom="8dp"
        android:background="@android:color/holo_red_dark" />

    <TextView
        android:text="Zacetna lokacija"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/textView2"
        app:layout_constraintRight_toLeftOf="@+id/guideline13"
        android:layout_marginEnd="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline12"
        app:layout_constraintBottom_toTopOf="@+id/guideline15"
        android:layout_marginBottom="8dp" />
</android.support.constraint.ConstraintLayout>